<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xyc.news.mapper.UserMapper">

<select id="findOneByUserName" parameterType="String" resultType="com.xyc.news.pojo.User">
        select * from user where username=#{username}
    </select>

    <insert id="insertUser" parameterType="User">
        insert into user (username,nickname,password,phone,role)
        values(#{username},#{nickname},#{password},#{phone},#{role})
    </insert>
</mapper>
